<%

vReferer = Request.QueryString("Referer")
vScript = Left(vReferer, 11)
vOK = "/Items01.asp"
vTF = (vScript = vOK)

if vReferer <> "" and NOT IsEmpty(vReferer) and NOT IsNULL(vReferer) and vTF then
   Session("Referer") = vReferer
else
   vReferer = Request.ServerVariables("SCRIPT_NAME") & "?" & Request.ServerVariables("QUERY_STRING")
   vScript = Left(vReferer, 11)
   vOK = "/Items01.asp"
   vTF = (vScript = vOK)
   if vReferer <> "" and NOT IsEmpty(vReferer) and NOT IsNULL(vReferer) and vTF then Session("Referer") = Server.URLEncode(vReferer)
End If

'response.write "Referer: " & vReferer & "<BR>"
'response.write "Session: " & Session("Referer")
%>
